<!doctype html>
<html>
	<head>
	<style>
	.personRow {color: green}
	</style>
	</head>
	<body>
		<h1>List of People in Our Database</h1>
		<ul>
			<% people.forEach( function( person ) { %>
			<li>
			<div class="personRow">
				<%= person.firstname %>
				<%= person.lastname %>
				<%= person.id %>
			</div>
			</li><br/>
			<% }) %>
		</ul>
	</body>
</html>